  <body>
    <form name="join_form" action="/?_method=PUT" method="POST">
      <p></p><p></p>
      <p></p><p></p>

      <div class="alert alert-primary" role="alert">
        장바구니 정보
      </div>

    <div class='product'>
      <% var sum = 0%>
      <% for (var i = 0; i < cartInfo.length; i ++) { %>
        <%sum += cartInfo[i].price %>
        <ol>
        <div>
        <img src=<%= cartInfo[i].imageLink %> width="200" height="200" alt="My Image">
        <h2>&nbsp;
          <%= cartInfo[i].name%></h2>      <p></p><p></p>
        <p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
          Price <%= cartInfo[i].price %></p>      <p></p><p></p>
        </div>
    </ol>
      <% } %>
    </div>
    </form>


    <form name="j" action="/order" method="POST">
